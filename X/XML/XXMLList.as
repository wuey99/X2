package X.XML {

//------------------------------------------------------------------------------------------
	public class XXMLList extends Object {
		private var m_list:XMLList;
		
//------------------------------------------------------------------------------------------
		public function XXMLList () {
			super();
		}

//------------------------------------------------------------------------------------------
		public function setupWithXMLList (__xmlList:XMLList):void {
			m_list = __xmlList;
		}
		
//------------------------------------------------------------------------------------------
		public function cleanup ():void {
		}

//------------------------------------------------------------------------------------------
		public function get (__index:Number):XXMLNode {
			var __xml:XML = m_list[__index];
			var __xxmlNode:XXMLNode = new XXMLNode ();
			__xxmlNode.setupWithXML (__xml);
			
			return __xxmlNode;
		}
		
//------------------------------------------------------------------------------------------
	}
	
//------------------------------------------------------------------------------------------
}